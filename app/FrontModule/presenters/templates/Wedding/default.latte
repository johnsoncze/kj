{block content}
<section class="PageHeaderNew">
	<div class="Container Container--wide">
		<div class="PageHeaderNew-desktop-bg Wedding-header">
			<video class="Wedding-header-video" autoplay="" muted="" loop="" playsinline>
				<source src="/assets/front/images/wedding/banner_video.mp4" type="video/mp4"/>
				Your browser does not support the video tag.
			</video>
		</div>
		<div class="PageHeaderNew-mobile-bg Wedding-header-mobile">
			<video class="Wedding-header-video-mobile" autoplay="" muted="" loop="" playsinline>
				<source src="/assets/front/images/wedding/banner_video_m.mp4" type="video/mp4"/>
				Your browser does not support the video tag.
			</video>			
		</div>

		<div class="Container Grid u-positionRelative">
			<div class="Category-relatedPageDefault Wedding-category-relatedPageDefault">
				<a href="{plink Page:detail, url => 'sjednani-osobni-schuzky'}">
					{_wedding.personalMeeting|noescape}
				</a>
			</div>
		</div>

				
		<div class="Container Grid u-mt--40 u-md-mt--0">
			<div class="Grid-cell u-size12of12">
				{control breadcrumb}

				<h1 class="Text-title u-mb--60 u-md-mb--100">{_wedding.engagementRings|noescape}</h1>
			</div>
		</div>
	</div>	
</section>

<section class="Background u-mb--80 u-md-mb--130">
	<div class="Container Grid u-px--0 u-md-px-20">
		<div class="Grid-cell u-size12of12 u-md-size7of12 u-pr--0 u-md-pr--60 u-mb--25 u-md-mb-0 u-px--0">
			<img src="/assets/front/images/wedding/dm.jpg" />
		</div>
		<div class="Grid-cell u-size12of12 u-md-size5of12 u-textCenter u-md-textLeft u-p--20 u-md-px-0">
			<h2 class="h-primary">{_wedding.title}</h2>
			<div class="Wedding-text">{_wedding.supportText|noescape}</div>
		</div>
		
		<div class="u-textCenter u-w--100-pcn u-mt--40 u-mb--80">
			<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Page:detail, url => 'sjednani-osobni-schuzky'}">{_wedding.btn_edvantage}</a>			
			<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Category:default, url => 'tolerance-zasnubni'}">{_wedding.btn_toleration}</a>			
			<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Category:default, url => 'zasnubni-prsteny'}">{_wedding.btn_all}</a>			
			<a class="u-textUppercase Button Button--arrow Button--gold u-mx--20" href="#schuzka">{_wedding.btn_meeting}</a>			
		</div>
	</div>
		
	<div class="Container Grid">
		<a href="#velikost" class="Grid-cell u-size12of12 u-md-size4of12 u-textCenter u-pr--0 u-md-pr--50 u-mb--50 u-md-mb--0">
			<img src="/assets/front/images/wedding/video_1_trigger.jpg" alt="" /><br />
			<div class="Wedding-article-preview-link">{_wedding.howEstimate.title|noescape}</div>
		</a>
		<a href="#zlato" class="Grid-cell u-size12of12 u-md-size4of12 u-textCenter u-px--0 u-md-px--30 u-mb--50 u-md-mb--0">
			<img src="/assets/front/images/wedding/video_2_trigger.jpg" alt="" /><br />
			<div class="Wedding-article-preview-link">{_wedding.goldColor.title|noescape}</div>
		</a>
		<a href="#proc-jk" class="Grid-cell u-size12of12 u-md-size4of12 u-textCenter u-pl--0 u-md-pl--50">
			<img src="/assets/front/images/wedding/video_3_trigger.jpg" alt="" /><br />
			<div class="Wedding-article-preview-link"><br />{_wedding.whyChoose.title|noescape}</div>
		</a>
	</div>		
		
</section>

<a name="velikost"></a>
<div class="Background Background--rose-lighter">
    <div class="Container u-pt--40 u-textCenter u-md-pt--70 u-pb--60 u-md-pb--70 u-px--0 u-md-px--20">
		<h2 class="h-primary u-textCenter u-mb--30 u-md-mb--50 u-px--20 u-md-px--0">{_wedding.howEstimate.title|noescape}</h2>

		{var $videoId = 'article1Video'}
		{capture $videoFrame}<iframe width="100%" height="400" id="videoPopup" src="https://www.youtube.com/embed/rdhsKik9mPU?version=3&enablejsapi=1" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>{/capture}
		{capture $videoTitle}{_wedding.howEstimate.title|noescape}{/capture}
		{include popup, id => $videoId, title => $videoTitle, content => $videoFrame}

		<div class="u-positionRelative">
			<img href="#{$videoId}" src="/assets/front/images/wedding/play.png" class="Wedding-play-trigger js-popup-opener"></a>
			<img src="/assets/front/images/wedding/video_1_trigger.jpg" class="u-mb--30 u-md-mb--60">
		</div>

		<div class="Wedding-article-wrapper Wedding-text u-mb--60 u-md-mb--80 u-px--20 u-md-px--0">
			<div class="u-textCenter"><b>{_wedding.howEstimate.topText}</b></div>
			<br /><br />
			<b>{_wedding.howEstimate.listHeader}</b><br />
			<ul class="Wedding-list">
				<li>{_wedding.howEstimate.list1}</li>
				<li>{_wedding.howEstimate.list2}</li>
				<li>{_wedding.howEstimate.list3}</li>
				<li>{_wedding.howEstimate.list4}</li>
			</ul>
			<br />
			{_wedding.howEstimate.bottomText|noescape}

		</div>

		<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Article:detail, url => 'jak-vybrat-nausnice-podle-tvaru-obliceje'}">{_wedding.readArticle}</a>
		<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Page:detail, url => 'sjednani-osobni-schuzky'}">{_wedding.howEstimate.helperDownload}</a>
		<a class="u-textUppercase Button Button--arrow Button--gold u-mx--20" href="#schuzka">{_wedding.howEstimate.meeting}</a>
    </div>
</div>
		

