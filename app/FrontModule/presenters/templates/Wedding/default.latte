			<img src="/assets/front/images/wedding/video_2_trigger.jpg" class="u-mb--60">
		</div>

		<div class="Wedding-article-wrapper Wedding-text u-mb--60 u-md-mb--60 u-px--20 u-md-px--0">{_wedding.goldColor.topText|noescape}</div>

		<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Article:detail, url => 'jak-vybrat-nausnice-podle-tvaru-obliceje'}">{_wedding.goldColor.edvantage}</a>
		<a class="u-textUppercase Button Button--arrow u-mx--20 u-mb--20" href="{plink Category:default, url => 'tolerance-zasnubni'}">{_wedding.goldColor.toleration}</a>
